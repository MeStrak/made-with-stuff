{
  "name": "@mestrak/made-with-stuff",
  "version": "1.0.2",
  "description": "A simple, fun Vue.js (and React using Vuera) component to display a customisable 'made with ‚ù§ by someone' tag",
  "main": "./dist/@mestrak/made-with-stuff.umd.js",
  "unpkg": "./dist/@mestrak/made-with-stuff.umd.min.js",
  "scripts": {
    "dev": "vue serve ./src/MadeWithStuff.vue",
    "build": "vue-cli-service build --target lib --name @mestrak/made-with-stuff ./src/MadeWithStuff.vue && cp ./src/made-with-stuff.d.ts ./dist/@mestrak/made-with-stuff.d.ts",
    "build-bundle": "vue-cli-service build --target lib --name @mestrak/made-with-stuff ./src/MadeWithStuff.vue && cp ./src/made-with-stuff.d.ts ./dist/@mestrak/made-with-stuff.d.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "MeStrak",
  "license": "MIT",
  "private": false,
  "types": "./dist/@mestrak/made-with-stuff.d.ts",
  "files": [
    "dist",
    "dist/*",
    "dist/**/*",
    "*.js",
    "*.css",
    "*.d.ts"
  ],
  "dependencies": {
    "@vue/cli": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "vue": "^2.6.11"
  },
  "devDependencies": {}
}
